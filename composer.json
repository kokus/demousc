{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "url": "https://github.com/jidrone/drall.git",
            "type": "git"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "composer/installers": "^2.0",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.4",
        "drupal/akamai": "^5.0@RC",
        "drupal/ape": "^1.6",
        "drupal/auto_entitylabel": "^3.0",
        "drupal/autologout": "^1.4",
        "drupal/bean_migrate": "^1.0",
        "drupal/chosen": "^4.0",
        "drupal/config_import_single": "^2.0",
        "drupal/config_pages": "^2.15",
        "drupal/core-composer-scaffold": "^10.1",
        "drupal/core-recommended": "^10.1",
        "drupal/ctools": "^4.0",
        "drupal/customerror": "^1.0@beta",
        "drupal/entity_legal": "^4",
        "drupal/environment_indicator": "^4.0",
        "drupal/extlink": "^1.7",
        "drupal/faqfield": "^7.1",
        "drupal/feeds": "^3.0@beta",
        "drupal/field_group": "^3.4",
        "drupal/fpa": "^4.0",
        "drupal/gin": "^3.0@RC",
        "drupal/google_tag": "^2.0",
        "drupal/gutenberg_uswds": "^1.0@beta",
        "drupal/honeypot": "^2.1",
        "drupal/html_processors": "1.0.x-dev@dev",
        "drupal/htmlpurifier": "^1.0",
        "drupal/image_effects": "^3.4",
        "drupal/image_link_formatter": "^2.1",
        "drupal/inline_entity_form": "^2.0@RC",
        "drupal/job_scheduler": "^4.0",
        "drupal/libraries": "^4.0",
        "drupal/linkit": "^6.1",
        "drupal/maxlength": "^2.1",
        "drupal/media_migration": "^1.0@alpha",
        "drupal/menu_item_extras": "^3.0",
        "drupal/menu_link_attributes": "^1.3",
        "drupal/menu_position": "^1.1",
        "drupal/metatag": "^2.0",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_tools": "^6.0",
        "drupal/migrate_upgrade": "^4.0",
        "drupal/module_filter": "^4.0",
        "drupal/paragraphs": "^1.16",
        "drupal/password_policy": "^4.0",
        "drupal/pathauto": "^1.12",
        "drupal/pathologic": "^2.0@alpha",
        "drupal/quick_node_clone": "^1.16",
        "drupal/redirect": "^1.9",
        "drupal/redis": "^1.7",
        "drupal/remote_stream_wrapper": "^2.0",
        "drupal/scheduler": "^2.0",
        "drupal/seckit": "^2.0",
        "drupal/sharethis": "^3.0",
        "drupal/stage_file_proxy": "^2.1",
        "drupal/taxonomy_manager": "^2.0",
        "drupal/transliterate_filenames": "^2.0",
        "drupal/usfedgov_google_analytics": "^2.0",
        "drupal/views_bulk_operations": "^4.2",
        "drupal/webform": "^6.2@RC",
        "drupal/xmlsitemap": "^1.5",
        "drush/drush": "^12.1",
        "mglaman/composer-drupal-lenient": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "cweagans/composer-patches": true,
            "mglaman/composer-drupal-lenient": true
        },
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "installer-types": ["bower-asset", "npm-asset"],
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/bean_migrate" : {
                "3369198 - Automated Drupal 10 compatibility fixes" : "https://www.drupal.org/files/issues/2023-11-27/3369198-4.patch"
            },
            "drupal/core" : {
                "Array filter issue" : "https://www.drupal.org/files/issues/2020-06-09/d7_field_instance-sort-3080034-6.patch",
                "Migrate views issue" : "https://www.drupal.org/files/issues/2023-04-19/3068442-28.patch"
            },
            "drupal/entity_legal" : {
                "3377137 - missing source" : "patches/entitylegal-3377137.patch"
            },
            "drupal/faqfield" : {
                "3012114 - Migrate process plugin?" : "https://www.drupal.org/files/issues/2020-11-19/add-migrate-support-3012114-4.patch"
            },
            "drupal/htmlpurifier" : {
                "3167171 - Drupal 7 â†’ 8/9 migration" : "https://www.drupal.org/files/issues/2020-08-25/3167171-2.patch"
            },
            "drupal/paragraphs" : {
                "3192993 - [migrate_upgrade compatibility] d7_field_instance upgrade fails due to redundant 'field_' prefix removal on bundle" : "https://www.drupal.org/files/issues/2021-01-29/paragraphs-n3192993-5.patch"
            }
        },
        "drupal-lenient": {
            "allowed-list": ["drupal/bean_migrate"]
        }
    },
    "require-dev": {
        "dmore/behat-chrome-extension": "^1.3",
        "dmore/chrome-mink-driver": "dev-main as 2.9.2",
        "drevops/behat-screenshot": "^1.1",
        "drupal/coder": "^8.3",
        "drupal/core-dev": "^10",
        "drupal/devel": "^5.0",
        "drupal/drupal-extension": "^5.0",
        "kint-php/kint": "^5.0",
        "mglaman/drupal-check": "^1.1",
        "mglaman/phpstan-drupal": "^1",
        "phpspec/prophecy-phpunit": "^2",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/var-dumper": "^6.2"
    }
}
